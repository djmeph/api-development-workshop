{
  "restartable": "rs",
  "ignore": [
    ".git/**/*",
    "node_modules/**/*",
    "package-lock.json"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "env": { "NODE_ENV": "development" },
  "ext": "js json"
}
